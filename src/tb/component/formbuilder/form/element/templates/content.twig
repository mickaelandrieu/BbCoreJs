<p>{{ element.getLabel() }}</p>

<div class="row clearfix">
    <div class="col-md-5">
        <img src="{{ element.image }}" style="width:80px;"/>
    </div>
  <div class="col-md-7">
      <p><button id="{{ element.getKey() }}_update_btn" class="btn btn-default-grey btn-sm">{{ 'edit'|trans }}</button></p>
  </div>
</div>

{% if element.getError() != null %}
    <span class="help-block">{{ element.getError() }}</span>
{% endif %}
